{
    "extends": "airbnb",
    "plugins": ["react", "babel"],
    "parser": "babel-eslint",
    "env": {
        "jest": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/prefer-stateless-function": 1,
        "react/destructuring-assignment": 1,
        "no-underscore-dangle": 0,
        "jsx-a11y/label-has-for": 1,
        "jsx-a11y/label-has-associated-control": 1,
        "consistent-return": 1
    }
}